{
  "name": "@terronex/engram",
  "version": "1.0.0",
  "description": "Engram - High-performance hierarchical, temporal, multi-modal AI memory format with neural-inspired architecture",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "docs": "typedoc src/index.ts",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "engram",
    "neural-memory",
    "ai",
    "memory",
    "memory-trace",
    "embeddings",
    "semantic-search",
    "vector-database",
    "hierarchical",
    "temporal",
    "multi-modal",
    "knowledge-graph",
    "machine-learning",
    "artificial-intelligence",
    "data-structures",
    "binary-format",
    "hnsw",
    "approximate-nearest-neighbor",
    "fast-search",
    "high-performance",
    "neuroscience"
  ],
  "author": {
    "name": "Terronex",
    "email": "contact@terronex.dev",
    "url": "https://terronex.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Terronex-dev/engram.git"
  },
  "homepage": "https://github.com/Terronex-dev/engram#readme",
  "bugs": {
    "url": "https://github.com/Terronex-dev/engram/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "hnswlib-node": "^3.0.0",
    "msgpackr": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.56.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
